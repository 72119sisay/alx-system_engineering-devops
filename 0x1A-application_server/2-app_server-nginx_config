user www-data;
worker_processes auto;
pid /run/nginx.pid;

events {
	worker_connections 768;
	# multi_accept on;
}

http {

        # gunicorn
	server {
            listen          80;
	    server_name     127.0.0.1 35.185.103.0;
	    location /airbnb-onepage/ {
		include proxy_params;
		proxy_pass http://localhost:5000/airbnb-onepage/;
	    }
	}
